def plus(x = [None,None]): 
    return [x[0], "plus"]
def minus(x = [None,None]):
    return [x[0], "minus"]
def times(x = [None,None]):
    return [x[0], "times"]
def divided_by(x = [None,None]):
    return [x[0], "divided"]
