def zero(x = [None,None]):
    if x[0] == None:
        return [0, None]
    if x[1] == "plus":
        return 0 + x[0]
    elif x[1] == "minus":
        return 0 - x[0]
    elif x[1] == "times":
        return 0 * x[0]
    elif x[1] == "divided":
        return 0 // x[0]
    return "Ooops"
                
def one(x = [None,None]): #your code here
    if x[0] == None:
        return [1, None]
    if x[1] == "plus":
        return 1 + x[0]
    elif x[1] == "minus":
        return 1 - x[0]
    elif x[1] == "times":
        return 1 * x[0]
